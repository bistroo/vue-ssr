    "@babel/parser": "^7.21.4",
    "@babel/traverse": "^7.21.4",
    "@babel/types": "^7.21.4",
    "@nuxt/devalue": "^2.0.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "compression": "^1.7.4",
    "express": "^4.18.2",
    "mri": "^1.2.0",
    "serve-static": "^1.15.0",
    "vite": "^4.2.1",
    "vue": "^3.2.47",
    "vue-router": "4"
  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "@types/node": "^18.15.11",
    "esbuild": "^0.17.16",
    "rollup": "^3.20.2",
    "rollup-plugin-dts": "^5.3.0",
    "rollup-plugin-esbuild": "^5.0.0",
    "typescript": "^5.0.4"
  }
