{
  "name": "vue-ssr",
  "version": "0.0.1",
  "type": "module",
  "license": "MIT",
  "author": "Youri van Mill",
  "description": "Minimalistic wrapper to run SSR Vue 3 apps",
  "bin": {
    "vue-ssr": "bin/vue-ssr.js"
  },
  "bugs": {
    "url": "https://github.com/yooouuri/vue-ssr/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/yooouuri/vue-ssr.git"
  },
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
